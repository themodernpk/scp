                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="35dp"
                                android:src="@drawable/grey_circle" />

                            <com.example.search.car.pools.FloatLabelLayout
