                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="15dp"
                                android:src="@drawable/blue_circle_small"
                                android:visibility="gone" />

                            <ImageView
                                android:id="@+id/iv_4"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="25dp"
                                android:src="@drawable/grey_circle" />

                            <RelativeLayout
                                android:id="@+id/l_city_create_list"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="40dp"
                                android:layout_toRightOf="@+id/view1" >

                                <TextView
                                    android:id="@+id/tv_2_create_list"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="fonts/AvenirLTStd_Book.otf"
                                    android:text="Selected City"
                                    android:textColor="#0087ca"
                                    android:textSize="13sp"
                                    android:textStyle="bold"
                                    android:visibility="gone" />

                                <TextView
                                    android:id="@+id/sp_sec_city"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/tv_2_create_list"
                                    android:text="Select City"
                                    android:textColor="#27272f"
                                    android:textSize="16sp" />

                                <ImageView
                                    android:id="@+id/imageView1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignBottom="@+id/tv_c_sec_city"
                                    android:layout_alignParentRight="true"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="40dp"
                                    android:src="@drawable/drop_down" />
                            </RelativeLayout>
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dgdf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <RelativeLayout
                            android:id="@+id/dgdf"
                            android:layout_width="fill_parent"
                            android:layout_height="60dp"
                            android:paddingLeft="15dp" >

                            <View
                                android:id="@+id/view1"
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_alignBottom="@+id/view2"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="20dp"
                                android:background="#ededed" />

                            <View
                                android:id="@+id/view2"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_alignLeft="@+id/fl1"
                                android:layout_alignParentBottom="true"
                                android:layout_marginRight="20dp"
                                android:background="#ededed" />

                            <ImageView
                                android:id="@+id/iv_05"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="20dp"
                                android:src="@drawable/blue_circle_small"
