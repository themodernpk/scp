<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.example.search.car.pools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical" >

    <LinearLayout
            android:id="@+id/l_search_1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerInParent="true"
            android:orientation="vertical" >

            <TextView
            android:id="@+id/et_sec_city"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="5dp"
            android:fontFamily="fonts/AvenirLTStd_Book.otf"
            android:gravity="center"
            android:text="Add Your Company"
            android:textColor="#27272f"
            android:textSize="20sp"
            android:textStyle="bold" />

            <View
                android:id="@+id/viewTop"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="15dp"
                android:background="#e9e9e9" />

            <ScrollView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp" >

                <LinearLayout
                    android:id="@+id/scroll_l"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/dgdf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <RelativeLayout
                            android:id="@+id/dgdf"
                            android:layout_width="fill_parent"
                            android:layout_height="60dp"
                            android:paddingLeft="15dp" >

                            <View
                                android:id="@+id/view1"
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_alignBottom="@+id/view2"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="20dp"
                                android:background="#ededed" />

                            <View
                                android:id="@+id/view2"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_alignLeft="@+id/fl1"
                                android:layout_alignParentBottom="true"
                                android:layout_marginRight="20dp"
                                android:background="#ededed" />

                            <ImageView
                                android:id="@+id/iv_01"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="20dp"
                                android:src="@drawable/blue_circle_small"
                                android:visibility="gone" />

                            <ImageView
                                android:id="@+id/iv_1"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="35dp"
                                android:src="@drawable/grey_circle" />

                            <com.example.search.car.pools.FloatLabelLayout
                                android:id="@+id/fl1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="35dp"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="15dp"
                                android:layout_toRightOf="@+id/iv_1"
                                app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.BlueTextSearch" >

                                <EditText
                                    android:id="@+id/et_company_name"
                                    android:layout_width="match_parent"
                                    android:layout_height="35dp"
                                    android:background="#00000000"
                                    android:hint="Company Name"
                                    android:inputType="textCapWords"
                                    android:paddingLeft="2.5dp"
                                    android:textColor="#27272f"
                                    android:textCursorDrawable="@drawable/cursor_color"
                                    android:textSize="16sp" />
                            </com.example.search.car.pools.FloatLabelLayout>
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dgdf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <RelativeLayout
                            android:id="@+id/dgdf"
                            android:layout_width="fill_parent"
                            android:layout_height="60dp"
                            android:paddingLeft="15dp" >

                            <View
                                android:id="@+id/view1"
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_alignBottom="@+id/view2"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="20dp"
                                android:background="#ededed" />

                            <View
                                android:id="@+id/view2"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_alignLeft="@+id/fl1"
                                android:layout_alignParentBottom="true"
                                android:layout_marginRight="20dp"
                                android:background="#ededed" />

                            <ImageView
                                android:id="@+id/iv_02"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="20dp"
                                android:src="@drawable/blue_circle_small"
                                android:visibility="gone" />

                            <ImageView
                                android:id="@+id/iv_2"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="35dp"
                                android:src="@drawable/grey_circle" />

                            <com.example.search.car.pools.FloatLabelLayout
                                android:id="@+id/fl1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="35dp"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="15dp"
                                android:layout_toRightOf="@+id/iv_2"
                                app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.BlueTextSearch" >

                                <EditText
                                    android:id="@+id/et_company_address"
                                    android:layout_width="match_parent"
                                    android:layout_height="35dp"
                                    android:background="#00000000"
                                    android:hint="Company Address"
                                    android:inputType="textPostalAddress"
                                    android:paddingLeft="2.5dp"
                                    android:textColor="#27272f"
                                    android:textCursorDrawable="@drawable/cursor_color"
                                    android:textSize="16sp" />
                            </com.example.search.car.pools.FloatLabelLayout>
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dgdf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <RelativeLayout
                            android:id="@+id/dgdf"
                            android:layout_width="fill_parent"
                            android:layout_height="60dp"
                            android:paddingLeft="15dp" >

                            <View
                                android:id="@+id/view1"
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_alignBottom="@+id/view2"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="20dp"
                                android:background="#ededed" />

                            <View
                                android:id="@+id/view2"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_alignParentBottom="true"
                                android:layout_marginLeft="40dp"
                                android:layout_marginRight="20dp"
                                android:layout_toRightOf="@+id/view1"
                                android:background="#ededed" />

                            <ImageView
