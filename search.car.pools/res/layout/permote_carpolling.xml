                                    android:inputType="number"
                                    android:paddingLeft="2.5dp"
                                    android:textColor="#27272f"
                                    android:textCursorDrawable="@drawable/cursor_color"
                                    android:textSize="16sp" />
                            </com.example.search.car.pools.FloatLabelLayout>
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dgdf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <RelativeLayout
                            android:id="@+id/dgdf"
                            android:layout_width="fill_parent"
                            android:layout_height="60dp"
                            android:paddingLeft="15dp" >

                            <View
                                android:id="@+id/view1"
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_alignBottom="@+id/view2"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="20dp"
                                android:background="#ededed" />

                            <View
                                android:id="@+id/view2"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_alignLeft="@+id/fl1"
                                android:layout_alignParentBottom="true"
                                android:layout_marginRight="20dp"
                                android:background="#ededed" />

                            <ImageView
                                android:id="@+id/iv_08"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="20dp"
                                android:src="@drawable/blue_circle_small"
                                android:visibility="gone" />

                            <ImageView
                                android:id="@+id/iv_8"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="35dp"
                                android:src="@drawable/grey_circle" />

                            <com.example.search.car.pools.FloatLabelLayout
                                android:id="@+id/fl1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="35dp"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="15dp"
                                android:layout_toRightOf="@+id/iv_8"
                                app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.BlueTextSearch" >

                                <EditText
                                    android:id="@+id/et_designation"
                                    android:layout_width="match_parent"
