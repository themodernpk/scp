                    android:id="@+id/iv_user_image_my_profile"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:background="#00000000" />
                <ImageView
                    android:id="@+id/iv_user_image_my_profile_gravatar"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:background="#00000000"
