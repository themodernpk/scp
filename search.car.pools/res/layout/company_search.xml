<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.example.search.car.pools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#fff"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/l_company_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/layout_search"
        android:gravity="center_vertical|center_horizontal"
        android:orientation="horizontal"
        android:padding="1dp"
        android:weightSum="10" >

        <EditText
            android:id="@+id/et_company_name_search"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_centerInParent="true"
            android:layout_weight="8"
            android:background="@drawable/search_company"
            android:ellipsize="start"
            
            android:hint="Search your company"
            android:imeOptions="actionDone"
            android:paddingLeft="10dp"
            android:textColor="#fff"
            android:textColorHint="#737377"
            android:textCursorDrawable="@drawable/cursor_color" />

        <LinearLayout
            android:id="@+id/l_search"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_below="@+id/l_company_name"
            android:layout_weight="2"
            android:background="@drawable/bbutton_search"
            android:gravity="center_vertical|center_horizontal" >

            <ImageView
                android:id="@+id/b_find_company"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerHorizontal="true"
                android:adjustViewBounds="true"
                android:layerType="software" />
        </LinearLayout>
    </LinearLayout>

    <ProgressBar
        android:id="@+id/progressBar1"
        style="?android:attr/progressBarStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp" />

    <!-- <android.support.v4.widget.SwipeRefreshLayout -->
