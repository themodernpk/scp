<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.example.search.car.pools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0087ca"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/iv_3_login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:adjustViewBounds="true"
        android:layerType="software"
        android:scaleType="centerCrop" />

    <LinearLayout
        android:id="@+id/l_search_1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@+id/tv_new_user"
        android:layout_centerInParent="true"
        android:layout_marginBottom="1dp"
        android:layout_marginTop="15dp"
        android:background="#00000000"
        android:orientation="vertical" >

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:id="@+id/scroll_l"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="5dp"
                    android:fontFamily="fonts/Znikomit.otf"
                    android:text="REGISTER NOW!"
                    android:textColor="#ebffff"
                    android:textSize="20dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/iv_company_logo"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginBottom="10dp"
                    android:layerType="software"
                    android:visibility="gone" />

                <RelativeLayout
                    android:id="@+id/L1_name"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="10dp"
                    android:background="#00000000"
                    android:orientation="vertical" >

                    <com.example.search.car.pools.FloatLabelLayout
                        android:id="@+id/fl_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.White" >

                        <EditText
                            android:id="@+id/et_user_name"
                            android:layout_width="fill_parent"
                            android:layout_height="35dp"
                            android:layout_margin="2.5dp"
                            android:background="#00000000"
                            android:hint="Name"
                            android:inputType="textPersonName|textCapWords"
                            android:nextFocusForward="@+id/et_user_password"
                            android:textColor="#d5ffff"
                            android:textColorHint="#d5ffff" />
                    </com.example.search.car.pools.FloatLabelLayout>

                    <View
                        android:id="@+id/view1"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/fl_1"
                        android:layout_marginTop="5dp"
                        android:background="#3bb0e9" />
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/dgdf"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="10dp"
                    android:background="#00000000"
                    android:orientation="vertical" >

                    <RelativeLayout
                        android:id="@+id/dgdf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <ImageView
                            android:id="@+id/iv_email_login"
                            android:layout_width="20dp"
                            android:layout_height="15dp"
                            android:layout_alignParentLeft="true"
                            android:layout_marginRight="15dp"
                            android:layout_marginTop="20dp"
                            android:layerType="software"
                            android:visibility="gone" />

                        <com.example.search.car.pools.FloatLabelLayout
                            android:id="@+id/fl_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/iv_email_login"
                            app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.White" >

                            <EditText
                                android:id="@+id/et_user_email"
                                android:layout_width="fill_parent"
                                android:layout_height="35dp"
                                android:layout_margin="2.5dp"
                                android:background="#00000000"
                                android:fontFamily="fonts/Znikomit.otf"
                                android:hint="Email"
                                android:inputType="textEmailAddress"
                                android:textColor="#d5ffff"
                                android:textColorHint="#d5ffff" />
                        </com.example.search.car.pools.FloatLabelLayout>
                    </RelativeLayout>

                    <View
                        android:id="@+id/view1"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="5dp"
                        android:background="#3bb0e9" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/dgdf"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="10dp"
                    android:background="#00000000"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/iv_pass_login"
                        android:layout_width="20dp"
                        android:layout_height="15dp"
                        android:layout_alignParentLeft="true"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        android:layerType="software"
                        android:visibility="gone" />

                    <com.example.search.car.pools.FloatLabelLayout
                        android:id="@+id/fl_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/iv_pass_login"
                        app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.White" >

                        <EditText
                            android:id="@+id/et_user_password"
                            android:layout_width="fill_parent"
                            android:layout_height="35dp"
                            android:layout_margin="2.5dp"
                            android:background="#00000000"
                            android:fontFamily="fonts/Znikomit.otf"
                            android:hint="Password"
                            android:inputType="textPassword"
                            android:textColor="#d5ffff"
                            android:textColorHint="#d5ffff" />
                    </com.example.search.car.pools.FloatLabelLayout>

                    <View
                        android:id="@+id/view1"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/fl_1"
                        android:layout_marginTop="5dp"
                        android:background="#3bb0e9" />
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/l1_login"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/l_1_"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10dp"
                        android:background="#00000000"
                        android:orientation="vertical" >

                        <com.example.search.car.pools.FloatLabelLayout
                            android:id="@+id/fl_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.White" >

                            <EditText
                                android:id="@+id/et_user_phone"
                                android:layout_width="fill_parent"
                                android:layout_height="35dp"
                                android:layout_margin="2.5dp"
                                android:background="#00000000"
                                android:fontFamily="fonts/Znikomit.otf"
                                android:hint="Phone"
                                android:inputType="number"
                                android:textColor="#d5ffff"
                                android:textColorHint="#d5ffff" />
                        </com.example.search.car.pools.FloatLabelLayout>

                        <View
                            android:id="@+id/view1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="#3bb0e9" />
                    </LinearLayout>

                    <RelativeLayout
                        android:id="@+id/rl1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:background="#00000000" >

                        <TextView
                            android:id="@+id/tv_1_login"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_marginTop="10dp"
                            android:background="#00000000"
                            android:fontFamily="fonts/Znikomit.otf"
                            android:gravity="center_vertical"
                            android:text="Selected City"
                            android:textColor="#d5ffff"
                            android:textSize="11sp"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tv_sec_city"
                            android:layout_width="fill_parent"
                            android:layout_height="35dp"
                            android:layout_below="@+id/tv_1_login"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_toLeftOf="@+id/iv_1"
                            android:background="#00000000"
                            android:fontFamily="fonts/Znikomit.otf"
                            android:gravity="center_vertical"
                            android:text="Select City"
                            android:textColor="#d5ffff"
                            android:textSize="18sp" />

                        <View
                            android:id="@+id/view1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tv_sec_city"
                            android:layout_marginTop="5dp"
                            android:background="#3bb0e9" />

                        <ImageView
                            android:id="@+id/iv_1"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/drop_down_white" >
                        </ImageView>
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/dgdf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10dp"
                        android:background="#00000000"
                        android:orientation="vertical" >

                        <com.example.search.car.pools.FloatLabelLayout
                            android:id="@+id/fl_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.White" >

                            <EditText
                                android:id="@+id/et_user_place"
                                android:layout_width="fill_parent"
                                android:layout_height="35dp"
                                android:layout_margin="2.5dp"
                                android:background="#00000000"
                                android:fontFamily="fonts/Znikomit.otf"
                                android:hint="Place"
                                android:inputType="textCapWords"
                                android:textColor="#d5ffff"
                                android:textColorHint="#d5ffff" />
                        </com.example.search.car.pools.FloatLabelLayout>

                        <View
                            android:id="@+id/view1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="#3bb0e9" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll2"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10dp"
                        android:background="#00000000"
                        android:orientation="vertical" >

                        <com.example.search.car.pools.FloatLabelLayout
                            android:id="@+id/fl_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:floatLabelTextAppearance="@style/TextAppearance.FLL.FloatLabel.White" >

                            <EditText
                                android:id="@+id/et_user_profession"
                                android:layout_width="fill_parent"
                                android:layout_height="35dp"
                                android:layout_margin="2.5dp"
                                android:background="#00000000"
                                android:fontFamily="fonts/Znikomit.otf"
                                android:hint="Profession"
                                android:inputType="textCapWords"
                                android:textColor="#d5ffff"
                                android:textColorHint="#d5ffff" />
                        </com.example.search.car.pools.FloatLabelLayout>

                        <View
                            android:id="@+id/view1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="#3bb0e9" />
                    </LinearLayout>

                    <RelativeLayout
                        android:id="@+id/rl2"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:background="#00000000" >

                        <TextView
                            android:id="@+id/tv_2_login"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_marginTop="10dp"
                            android:background="#00000000"
                            android:gravity="center_vertical"
                            android:text="Selected Year of Birth"
                            android:textColor="#d5ffff"
                            android:textSize="11sp"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tv_sec_brith"
                            android:layout_width="fill_parent"
                            android:layout_height="35dp"
                            android:layout_below="@+id/tv_2_login"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="10dp"
                            android:fontFamily="fonts/Znikomit.otf"
                            android:gravity="center_vertical"
                            android:text="Select Year of Birth"
                            android:textColor="#d5ffff"
                            android:textSize="18sp" />

                        <View
                            android:id="@+id/view1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tv_sec_brith"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="#3bb0e9" />

                        <ImageView
                            android:id="@+id/iv_2"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/drop_down_white" >
                        </ImageView>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rl3"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:background="#00000000" >

                        <TextView
                            android:id="@+id/tv_3_login"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_marginTop="5dp"
                            android:background="#00000000"
                            android:fontFamily="fonts/Znikomit.otf"
                            android:gravity="center_vertical"
                            android:text="Selected Gender"
                            android:textColor="#d5ffff"
                            android:textSize="18sp" />

                        <RadioGroup
                            android:id="@+id/rg_gender"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/tv_3_login"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="15dp"
                            android:orientation="horizontal"
                            android:weightSum="10" >

                            <RadioButton
                                android:id="@+id/radio0"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="5"
                                android:button="@drawable/custom_radio_button_login"
                                android:paddingLeft="10dp"
                                android:text="Male"
                                android:textColor="#d5ffff" />

                            <RadioButton
                                android:id="@+id/radio1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="5"
                                android:button="@drawable/custom_radio_button_login"
                                android:paddingLeft="10dp"
                                android:text="Female"
                                android:textColor="#d5ffff" />
                        </RadioGroup>

                        <View
                            android:id="@+id/view1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/rg_gender"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="#3bb0e9" />
                    </RelativeLayout>
                </LinearLayout>

                <Button
                    android:id="@+id/b_user_register"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/bbutton"
                    android:fontFamily="fonts/Znikomit.otf"
                    android:text="REGISTER"
                    android:textColor="#d5ffff"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <RelativeLayout
                    android:id="@+id/l_forgot"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                    android:layout_marginRight="20dp" >

                    <TextView
                        android:id="@+id/tv_forgot_pass"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="Forgot Password?"
                        android:textColor="#ededed"
                        android:textSize="12sp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/l_12"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="10dp" >

                    <View
                        android:id="@+id/view1"
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/tv_OR"
                        android:layout_weight="1"
                        android:background="#3bb0e9" />

                    <TextView
                        android:id="@+id/tv_OR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_weight="1"
                        android:text="OR"
                        android:textColor="#d5ffff"
                        android:textSize="16sp" />

                    <View
                        android:id="@+id/view1"
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/tv_OR"
                        android:layout_weight="1"
                        android:background="#3bb0e9" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/tv_new_user"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/button_dark_blue"
                    android:fontFamily="fonts/Znikomit.otf"
                    android:gravity="center"
                    android:text="LOGIN"
                    android:textColor="#d5ffff"
                    android:textSize="21sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</RelativeLayout>